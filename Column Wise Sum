#include<iostream>
using namespace std;


int main(){

       /*  Read input as specified in the question.
	* Print output as specified in the question.
	*/int m,n;
    cin>>m>>n;
    int arr[m][n];
    int i,j,k;
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
            cin>>arr[i][j];
    }
    long int sum=0;
    for(j=0;j<n;j++)
    { sum=0;
    for(i=0;i<m;i++)
    {
        sum=sum+arr[i][j];
    }
        cout<<sum<<" ";
    }
    
    
    
    
    
  
}


