#include<iostream>
using namespace std;

int main() {
	// Write your code here
	long int n,result=0;
    int i,j;
    int arr[100]={0};
    cin>>n;
    
    if(n==0)cout<<"0";
    else
    {
    for(i=0;n>0;i++)
    {
        arr[i]=n%2;
        n=n/2;
    }
    for(j=0;j<i;j++)
        cout<<arr[i-j-1];
}
}
